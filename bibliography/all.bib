@article{Furukawa2014,
abstract = {We study the ground-state phase diagram of two-dimensional two-component (or pseudospin-1/2) Bose gases in antiparallel synthetic magnetic fields, in the space of the total filling factor \$\backslash nu\_\backslash mathrm\{tot\}\$ and the ratio of the intercomponent coupling \$g\_\{\backslash uparrow\backslash downarrow\}\$ to the intracomponent one \$g>0\$. This time-reversal-invariant setting represents a bosonic analogue of spin Hall systems. Using exact diagonalization, we find that (fractional) quantum spin Hall states composed of two nearly independent quantum Hall states are remarkably robust and persist even when \$g\_\{\backslash uparrow\backslash downarrow\}\backslash approx g\$. For \$g\_\{\backslash uparrow\backslash downarrow\}=-g\$, we obtain exact many-body ground states in which particles of different spin states form pairs. This gives the exact critical line beyond which the system collapses.},

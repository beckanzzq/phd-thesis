month = jan,
number = {3},
pages = {168--173},
publisher = {Nature Publishing Group},
shorttitle = {Nat Phys},
title = {{A large-scale quantum simulator on a diamond surface at room temperature}},
url = {http://arxiv.org/abs/1208.2874 http://dx.doi.org/10.1038/nphys2519 http://www.nature.com/doifinder/10.1038/nphys2519},
volume = {9},
